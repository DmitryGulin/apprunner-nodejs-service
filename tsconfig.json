{
    "compilerOptions": {
      "target": "ES2022",
      "strict": true,
      "lib": [
          "ES2022"
      ],
      "preserveConstEnums": true,
      "noEmit": true,
      "sourceMap": false,
      "module":"ES2022",
      "moduleResolution":"node",
      "esModuleInterop": true, 
      "skipLibCheck": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,  
    },
    "exclude": ["node_modules", "**/*.test.ts"]
}
